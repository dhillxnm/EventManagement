@model List<EventTable>
<h2>Events</h2>

<table class="table">
    <thead>
        <tr>
            <th>EventName</th>
            <th>EventLocation</th>
            <th>EventParticipants</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var events in Model)
        {
            <tr>
                <td>@events.EventName</td>
                <td>@events.EventLocation</td>
                <td>@events.EventParticipants</td>
                <td>
                    <a href="/EventTable/Delete/@events.EventId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>